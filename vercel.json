{
  "version": 2,
  "builds": [
    {
      "src": "next.config.js",
      "use": "@vercel/next"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NEXT_PUBLIC_ENV": "production"
    }
  },
  "environments": {
    "development": {
      "build": {
        "env": {
          "NODE_ENV": "development",
          "NEXT_PUBLIC_ENV": "development"
        }
      }
    }
  }
}
